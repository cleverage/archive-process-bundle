{
  "name": "cleverage/archive-process-bundle",
  "description": "Dedicated bundle for Archive dependencies for the Process Bundle",
  "keywords": [
    "process",
    "task",
    "etl",
    "transformation",
    "archive",
    "zip",
    "unzip"
  ],
  "homepage": "https://github.com/cleverage/archive-process-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Joubert",
      "email": "njoubert@clever-age.com",
      "role": "Lead Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "CleverAge\\ArchiveProcessBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CleverAge\\ArchiveProcessBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-zip": "*",
    "cleverage/process-bundle": "^4.0.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "*",
    "phpstan/extension-installer": "*",
    "phpstan/phpstan": "*",
    "phpstan/phpstan-symfony": "*",
    "phpunit/phpunit": "<10.0",
    "rector/rector": "*",
    "roave/security-advisories": "dev-latest",
    "symfony/test-pack": "^1.1"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  }
}
